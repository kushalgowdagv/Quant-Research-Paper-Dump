    function doProjectsDBSearch(strActionURL){        
        var searchValue = document.getElementById('qterm').value;
        
        
        if(searchValue== ''){            
            return false;
        }else{
            searchValue=searchValue.trim();
            currentURL = strActionURL ;
            var queryParam = "searchTerm=" + encodeURIComponent(searchValue);
            
            if($("._loop_primary_checkbox").is(":checked")){
                currentURL = $("._loop_primary_checkbox").attr("data-url");
                queryParam = "qterm=" + encodeURIComponent(searchValue) + "&srce=both";
            }           
            window.location.href = currentURL + '?' + queryParam;
        } 
    }
    
    $('#search-type').change(function() {		
        if($(this).is(":checked")) {                  
            $('.project-advancesearch a').attr('href', $(this).attr("procurement-url"));
            $('#procurements-browse-data').removeClass('hide');
            $('#projects-browse-data').addClass('hide');
        } else {
            $('.project-advancesearch a').attr('href', $(this).attr("project-url"));
            $('#procurements-browse-data').addClass('hide');
            $('#projects-browse-data').removeClass('hide');
        }          
        
    });
    
</script>


<script>
    function invokeListing(strActionURL){
        var langexact = "";
        var searchValue = document.getElementById('qterm').value; 
        var defaultValue = "Search+Documents";
        var listingType = $("input[name='lType']:checked").val();
        var qterm= "";
        var strQterm = "";
		if (strActionURL.indexOf('?') > -1)
			strQterm = "&qterm=";
        else
			strQterm = "?qterm=";


        
        if(listingType !== 'all')
			qterm= "lang_exact="+langexact;

        if(searchValue== ''){ 
            //strActionURL = strActionURL.replace('http','https');
            // window.location.href = strActionURL + encodeURIComponent('?qterm=Search Documents').replace(/%20/g,'+');
            if(qterm == '')
            window.location.href = strActionURL + strQterm +defaultValue;
            else
			  window.location.href = strActionURL + strQterm +defaultValue+'&'+qterm;
        } else {
            searchValue=searchValue.trim();
            currentURL = strActionURL ;
            searchValue = searchValue.split(' ').join('+');
            var queryParam = "qterm=" + searchValue;  
             if(qterm == '')
            window.location.href = currentURL + strQterm + searchValue;
            else
            window.location.href = currentURL + strQterm + searchValue+"&"+qterm;
        }
    }
</script>
